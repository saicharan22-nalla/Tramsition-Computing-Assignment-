<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Checklist Results</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <h1>Checklist Evaluation</h1>
  <table>
    <thead>
      <tr>
        <th>Rule</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <% results.forEach((result) => { %>
        <tr>
          <td><%= result.name %></td>
          <td><%= result.passed ? "Passed" : "Failed" %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
